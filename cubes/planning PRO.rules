## FORMAT==100

#NAME # Rule File for 'Planning PRO' cube. #

SKIPCHECK ;



#======================================================================================
[{'Actuals'}, 'USD Current Plan Rate'] = N:
#======================================================================================
    ['USD As Reported']
    * DB( 'ForEx Rates - Allocation Percentages - Revenue', !Scenario
    , ELPAR ('PMBU', !PMBU, 1)
    , 'Enterprise', !'Market Area', 'All Currencies', SubSt(!'Fiscal Quarter',4,4)
    , 'Weighted_FX_Alloc_Calc'  )
    ;


#======================================================================================
[{'Actuals'}, 'Prelim USD Current Plan Rate'] = N:
#======================================================================================
    ['USD As Reported']
    * DB( 'ForEx Rates - Allocation Percentages - Revenue', !Scenario
    , ELPAR ('PMBU', !PMBU, 1)
    , 'Enterprise', !'Market Area', 'All Currencies', SubSt(!'Fiscal Quarter',4,4)
    , 'Weighted_FX_Alloc_Calc'  )
    ;

#======================================================================================
[{'Actuals'}, 'USD Current QRF Rate'] = N:
#======================================================================================
    ['USD Current Plan Rate']
    * DB( 'ForEx Rates - Allocation Percentages - Revenue', !Scenario
    , ELPAR ('PMBU', !PMBU, 1)
    , 'Enterprise', !'Market Area', 'All Currencies', SubSt(!'Fiscal Quarter',4,4)
    , 'Weighted_FX_Alloc_Calc'  )
    ;

