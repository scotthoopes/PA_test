FEEDERS ;
#<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<

['Actuals', 'USD As Reported'] =>
    ['Actuals', 'USD Current Plan Rate']
  , ['Actuals', 'Prelim USD Current Plan Rate'];

['Actuals','USD Current Plan Rate'] =>
    ['Actuals', 'USD Current QRF Rate'] ;

['Actuals','Prelim USD Current Plan Rate'] =>
    ['Actuals', 'Prelim USD Current QRF Rate'] ;

[ {'Corp FPA Forecast'}, 'USD Current Plan Rate'] =>
    ['Corp FPA Forecast', 'USD Current QRF Rate']
  , ['Corp FPA Forecast', 'Prelim USD Current QRF Rate'] ;

[ {'Forecast1'}, 'USD Current Plan Rate'] =>
    ['Forecast1', 'USD Current QRF Rate']
  , ['Forecast1', 'Prelim USD Current QRF Rate'] ;
  
  [ {'Forecast4'}, 'USD Current Plan Rate'] =>
    ['Forecast4', 'USD Current QRF Rate']
  , ['Forecast4', 'Prelim USD Current QRF Rate'] ;



['Rocke Backup', 'USD As Reported'] =>
    ['Rocke Backup', 'USD Current Plan Rate']
  , ['Rocke Backup', 'Prelim USD Current Plan Rate'];

['Rocke Backup','USD Current Plan Rate'] =>
    ['Rocke Backup', 'USD Current QRF Rate'] ;

['Rocke Backup','Prelim USD Current Plan Rate'] =>
    ['Rocke Backup', 'Prelim USD Current QRF Rate'] ;