    , 'Enterprise', !'Market Area', 'All Currencies', SubSt(!'Fiscal Quarter',4,4)
    , 'Weighted_FX_Alloc_Calc'  )
    ;


#======================================================================================
[{'Forecast1', 'Forecast4', 'Corp FPA Forecast'}, 'USD Current QRF Rate'] = N:
#======================================================================================
    ['USD Current Plan Rate']
    * DB( 'ForEx Rates - Allocation Percentages - Revenue', DB ( 'Global Parameters', 'Current QRF Name', 'Value String' )
    , ELPAR ('PMBU', !PMBU, 1)
    , 'Enterprise', !'Market Area', 'All Currencies', SubSt(!'Fiscal Quarter',4,4)
    , 'Weighted_FX_Alloc_Calc'  )
    ;



